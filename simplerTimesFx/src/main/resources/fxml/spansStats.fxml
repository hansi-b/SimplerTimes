<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml"
	xmlns="http://javafx.com/javafx"
	fx:controller="org.hansib.simplertimes.fx.SpansStatsController">
	<HBox>
		<Button fx:id="monthBack" />
		<Button fx:id="weekBack" />
		<Button fx:id="today" />
		<Button fx:id="weekForward" />
		<Button fx:id="monthForward" />
	</HBox>
	<TableView fx:id="spansStats" prefWidth="600"
		HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
	</TableView>
</VBox>